{
  "name": "@salesforce/plugin-analytics",
  "version": "1.12.0",
  "author": "Salesforce",
  "description": "Command usage and error analytics for the Salesforce CLI",
  "engines": {
    "node": ">=8.4.0"
  },
  "files": [
    "/lib",
    "/processes",
    "/oclif.manifest.json"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "hooks": {
      "init": "./lib/hooks/analyticsInit.js",
      "prerun": "./lib/hooks/analyticsPrerun.js"
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "dependencies": {
    "@oclif/command": "~1.6.0",
    "@oclif/errors": "~1.2.2",
    "@oclif/parser": "^3.8.4",
    "@salesforce/command": "^2.2.0",
    "@salesforce/core": "^2.1.6",
    "@salesforce/kit": "^1.2.1",
    "@salesforce/telemetry": "~2.0.2",
    "@salesforce/ts-types": "~1.2.0",
    "debug": "4.1.1",
    "fs-extra": "7.0.1",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@oclif/config": "1.13.3",
    "@oclif/dev-cli": "1.22.2",
    "@oclif/plugin-help": "1.2.11",
    "@oclif/test": "1.2.5",
    "@salesforce/dev-config": "~1.5.0",
    "@salesforce/ts-sinon": "~1.1.0",
    "@types/chai": "4.1.7",
    "@types/debug": "0.0.31",
    "@types/fs-extra": "5.1.0",
    "@types/jsforce": "1.9.7",
    "@types/mocha": "5.2.7",
    "@types/node": "10.14.13",
    "chai": "4.2.0",
    "globby": "8.0.2",
    "husky": "^4.2.1",
    "mocha": "5.2.0",
    "nyc": "13.3.0",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "sinon": "5.1.1",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typescript": "3.8.3",
    "xunit-file": "1.0.0"
  },
  "repository": "salesforcedx/sfdx-plugin-analytics",
  "scripts": {
    "clean": "rm -rf lib *xunit.xml && yarn postpack",
    "postpack": "rm -f oclif.manifest.json",
    "pretest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "yarn prepare",
    "prepare": "yarn clean && tsc && tslint -p . -t stylish && oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\" -R xunit-file",
    "version": "oclif-dev readme && git add README.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "yarn test"
    }
  }
}
