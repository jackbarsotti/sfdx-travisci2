{
  "name": "@salesforce/sfdx-trust",
  "description": "Libraries and utilities For SFDX trust.",
  "version": "3.0.7",
  "author": "Tim Noonan @tnoonan-salesforce",
  "bugs": "https://github.com/tnoonan@salesforce.com/sfdx-trust/issues",
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@oclif/errors": "^1",
    "@salesforce/command": "^2.1.3",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^1",
    "@salesforce/core": "^2.1.4",
    "@salesforce/kit": "^1",
    "mkdirp": "0.5.1",
    "cli-ux": "^5.3.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^1",
    "@salesforce/dev-config": "^1",
    "@types/chai": "4.1.3",
    "@types/jsforce": "1.8.10",
    "@types/lodash": "4.14.108",
    "@types/mkdirp": "^0.5.2",
    "@types/fs-extra": "^5.0.5",
    "@types/mocha": "5.2.0",
    "@types/node": "10.14.3",
    "@types/request": "2.47.0",
    "@types/sinon": "^7.0.10",
    "chai": "4.1.2",
    "docdash": "0.4.0",
    "globby": "8.0.1",
    "istanbul": "v1.1.0-alpha.1",
    "jsdoc": "^3.6.2",
    "mocha": "5.1.1",
    "shelljs": "0.8.1",
    "sinon": "7.2.7",
    "source-map-support": "0.5.3",
    "ts-node": "6.0.1",
    "tslint": "5.11.0",
    "typescript": "3.0.3",
    "xunit-file": "1.0.0"
  },
  "engines": {
    "node": ">=8.4.0"
  },
  "files": [
    "oclif.manifest.json",
    "/dist/**/*.js",
    "/dist/**/*.ts",
    "!/dist/test/**/*.*",
    "/messages"
  ],
  "homepage": "https://github.com/tnoonan@salesforce.com/sfdx-trust",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./dist/commands",
    "hooks": {
      "plugins:preinstall:verify:signature": [
        "./dist/hooks/verifyInstallSignature.js"
      ]
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "tnoonan-salesforce/sfdx-trust",
  "scripts": {
    "prestart": "./node_modules/.bin/tsc",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "npm run prepare",
    "prepare": "rm -rf dist && tsc && oclif-dev manifest",
    "test": "mocha --forbid-only test/**/*.test.ts",
    "build": "npm run prepare && tslint -p test -t checkstyle -o checkstyle.xml",
    "test-with-coverage": "npm run build && scripts/unit-test-with-coverage.js"
  }
}
