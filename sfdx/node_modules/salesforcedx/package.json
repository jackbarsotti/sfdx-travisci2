{
    "name": "salesforcedx",
    "version": "49.2.3",
    "description": "",
    "main": "index.js",
    "files": [
        "npm-shrinkwrap.json",
        "oclif.manifest.json",
        "lib/**/*.js",
        "commands/**/*.js"
    ],
    "oclif": {
        "commands": "./commands",
        "plugins": [
            "salesforce-alm",
            "salesforcedx-templates",
            "@salesforce/sfdx-plugin-lwc-test",
            "@salesforce/plugin-custom-metadata"
        ]
    },
    "dependencies": {
        "@salesforce/plugin-custom-metadata": "^1.0.7",
        "@salesforce/sfdx-plugin-lwc-test": "0.1.7",
        "salesforce-alm": "49.3.0",
        "salesforce-lightning-cli": "3.0.0",
        "salesforcedx-templates": "48.32.0"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1.21.2",
        "bluebird": "3.5.0",
        "chai": "3.5.0",
        "chai-as-promised": "5.3.0",
        "chai-fs": "1.0.0",
        "chai-string": "1.2.0",
        "eslint": "3.2.2",
        "eslint-config-airbnb-base": "5.0.1",
        "eslint-plugin-import": "1.13.0",
        "fs-extra": "0.30.0",
        "lodash": "4.11.1",
        "mocha": "2.4.5",
        "remarkable": "1.7.1",
        "salesforcedx-shell-utils": "1.0.20",
        "sinon": "1.17.4",
        "xunit-file": "1.0.0"
    },
    "scripts": {
        "eslint": "node_modules/eslint/bin/eslint.js commands test index.js",
        "install": "node ./index.js",
        "postpack": "rm -f oclif.manifest.json",
        "prepack": "node_modules/.bin/oclif-dev manifest",
        "test": "_mocha --recursive test/unit",
        "platform-cli-smoke-tests": "_mocha -t 610000 -R xunit-file test/smoke/platform_cli.js",
        "mdapi-cli-smoke-tests": "_mocha -t 610000 -R xunit-file test/smoke/mdapi_cli.js",
        "pkg-smoke-tests": "_mocha -b -t 0 -R xunit-file test/smoke/package.js",
        "pkg1-smoke-tests": "_mocha -t 610000 -R xunit-file test/smoke/package1.js",
        "pkg-smoke-tests-local": "_mocha -b -t 0 test/smoke/package.js"
    },
    "author": "Salesforce",
    "license": "BSD-3-Clause",
    "sfdx": {
        "publicKeyUrl": "https://developer.salesforce.com/media/salesforce-cli/sfdx-cli-03032020.crt",
        "signatureUrl": "https://developer.salesforce.com/media/salesforce-cli/signatures/salesforcedx-49.2.3.sig"
    }
}